   "source": [
    "def power(base,exp):\n",
    "    if exp == 0:\n",
    "        return 1\n",
    "    if(exp==1):\n",
    "        return(base)\n",
    "    if(exp!=1):\n",
    "        return (base*power(base,exp-1))\n",
    "\n",
    "print(power(4,2))"
   ]
